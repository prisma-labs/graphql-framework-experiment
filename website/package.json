{
  "name": "nexus-docs",
  "description": "Nexus Docs",
  "version": "1.0.0",
  "dependencies": {
    "@mdx-js/mdx": "1.6.1",
    "@mdx-js/react": "1.6.1",
    "@philpl/buble": "0.19.7",
    "@prisma/client": "2.0.0-beta.4",
    "@reach/router": "1.3.3",
    "algoliasearch": "4.2.0",
    "babel-plugin-styled-components": "1.10.7",
    "downshift": "5.2.5",
    "gatsby": "2.21.12",
    "gatsby-image": "2.4.1",
    "gatsby-plugin-algolia": "0.5.0",
    "gatsby-plugin-catch-links": "2.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-mdx": "1.2.6",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-remove-trailing-slashes": "2.3.1",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.6.1",
    "gatsby-plugin-sitemap": "2.4.2",
    "gatsby-plugin-smoothscroll": "1.1.0",
    "gatsby-plugin-styled-components": "3.3.1",
    "gatsby-plugin-typescript": "2.4.1",
    "gatsby-remark-autolink-headers": "2.3.1",
    "gatsby-remark-copy-linked-files": "2.3.4",
    "gatsby-remark-images": "3.3.1",
    "gatsby-remark-prismjs": "3.5.1",
    "gatsby-remark-rewrite-relative-links": "1.0.8",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-source-remote-file": "0.1.9",
    "gatsby-transformer-remark": "2.8.6",
    "gatsby-transformer-sharp": "2.5.1",
    "is-absolute-url": "3.0.3",
    "lodash": "4.17.19",
    "polished": "3.6.1",
    "prism-react-renderer": "1.1.0",
    "prismjs": "^1.15.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-ga": "2.7.0",
    "react-helmet": "6.0.0",
    "react-hooks-global-state": "1.0.0",
    "react-hooks-testing-library": "0.3.8",
    "react-instantsearch-dom": "6.4.0",
    "react-live": "2.2.2",
    "react-loadable": "5.5.0",
    "react-sticky": "6.0.3",
    "react-testing-library": "6.1.2",
    "smooth-scroll": "16.1.3",
    "styled-components": "4.4.1",
    "styled-components-ts": "0.0.15",
    "write": "2.0.0"
  },
  "keywords": [
    "nexus"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build --prefix-paths --log-pages",
    "dev": "gatsby develop",
    "serve": "yarn build && gatsby serve --prefix-paths -o",
    "prettify": "prettier --write \"**/*.{js,mdx,json,md,ts,tsx,yml,toml,graphql,css}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@mdx-js/loader": "0.20.3",
    "@mdx-js/tag": "0.20.3",
    "@prisma/cli": "2.0.0-beta.4",
    "@types/jest": "26.0.8",
    "@types/react-helmet": "5.0.15",
    "@types/styled-components": "4.4.3",
    "gatsby-remark-sectionize": "1.0.0",
    "jest": "24.9.0",
    "jest-dom": "3.5.0",
    "loader-utils": "1.4.0",
    "prettier": "1.19.1",
    "ts-jest": "24.3.0",
    "tslint": "5.20.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
