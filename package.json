{
  "name": "pumpkins",
  "version": "0.0.0-sha.e03f7b2",
  "author": "Prisma Labs",
  "bin": {
    "pumpkins": "dist/cli/index.js"
  },
  "bugs": "https://github.com/prisma-labs/pumpkins/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "apollo-server-express": "^2.9.7",
    "debug": "^4.1.1",
    "graphql": "^14.5.8",
    "nexus": "^0.12.0-rc.4",
    "nexus-prisma": "^0.5.0",
    "simple-git": "^1.126.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslib": "^1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.22",
    "@types/node": "^10",
    "@types/tmp": "^0.1.0",
    "chai": "^4",
    "doctoc": "^1.4.0",
    "fs-jetpack": "^2.2.3",
    "globby": "^10",
    "jest": "^24.9.0",
    "nyc": "^14",
    "prettier": "^1.18.2",
    "tmp": "^0.1.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/dist",
    "/src",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/prisma-labs/pumpkins",
  "license": "MIT",
  "main": "dist/index.js",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "oclif": {
    "commands": "dist/cli/commands",
    "bin": "pumpkins",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "prisma-labs/pumpkins",
  "scripts": {
    "publish:sha": "yarn publish --tag sha --no-git-tag-version --new-version \"0.0.0-sha.$(git show -s head --format='%h')\"",
    "format": "prettier --write '*/**/*.ts'",
    "build:doc": "oclif-dev readme && doctoc --notitle README.md",
    "clean": " rm -rf oclif.manifest.json dist",
    "postpack": "yarn -s clean",
    "prepack": "yarn -s clean && tsc -b && oclif-dev manifest && yarn -s build:doc",
    "test": "jest",
    "test:dev": "jest --watch",
    "dev": "tsc -b -w",
    "example": "yarn ts-node ./example/index.ts"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
