{
  "name": "pumpkins",
  "version": "0.0.0",
  "author": "Prisma Labs",
  "bin": {
    "pumpkins": "./bin/run"
  },
  "bugs": "https://github.com/prisma-labs/pumpkins/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "apollo-server-express": "^2.9.7",
    "graphql": "^14.5.8",
    "nexus": "^0.12.0-rc.4",
    "nexus-prisma": "^0.5.0",
    "tslib": "^1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/jest": "^24.0.22",
    "@types/node": "^10",
    "chai": "^4",
    "doctoc": "^1.4.0",
    "globby": "^10",
    "jest": "^24.9.0",
    "nyc": "^14",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/prisma-labs/pumpkins",
  "license": "MIT",
  "main": "lib/index.js",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "oclif": {
    "commands": "./lib/cli",
    "bin": "pumpkins",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "prisma-labs/pumpkins",
  "scripts": {
    "format": "prettier --write '*/**/*.ts'",
    "build:doc": "oclif-dev readme && doctoc --notitle README.md",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && yarn -s build:doc",
    "test": "jest",
    "version": "yarn -s build:doc && git add README.md",
    "dev": "tsc -b -w",
    "example": "yarn ts-node ./example/index.ts"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "types": "lib/index.d.ts"
}
